			{
				"src": "bower_components/chosen/*.png",
				"dest": "./assets/css/"
			}
		],
		"styles": [
			"src/sass/**/*.scss"
		],
		"scripts": [
			"src/js/init.js",
			"src/js/geonorge-app.js",
			"src/js/geonorge-searchoptions.js",
			"src/js/geonorge-searchTopController.js",
			"src/js/geonorge-baseUrl.js",
			"src/js/geonorge-common.js",
			"src/js/geonorge-menuTopController.js",
			"src/js/shopping-cart.js",
			"src/js/helpers.js"
		],
		"fonts": [
			"src/fonts/**/*.{ttf,woff,woff2,eot,svg}"
		],
		"images": [
			"src/images/**/*.{png,svg,gif,jpg}"
		],
		"html": [
			"src/partials/*.html"
		],
		"cshtml": [
			"src/partials/*.cshtml"
		]
		
	}
}
